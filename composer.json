{
    "name": "launchpad-wp",
    "type": "project",
    "license": "GPL",
    "description": "A simple production process for Timber and Advanced Custom Fields based WordPress theme development.",
    "homepage": "https://github.com/jdaio/launchpad-wp",
    "authors": [{
        "name": "Jamal Ali-Mohammed",
        "email": "jamal@digitalheat.co",
        "homepage": "https://github.com/jdaio"
    }],
    "keywords": [
        "acf", "timber", "twig", "wordpress", "wp"
    ],
    "support": {
        "issues": "https://github.com/jdaio/launchpad-wp/issues"
    },
    "repositories": [{
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "advanced-custom-fields/advanced-custom-fields-pro",
                "version": "5.8.7",
                "type": "wordpress-plugin",
                "dist": {
                    "type": "zip",
                    "url": "https://connect.advancedcustomfields.com/index.php?a=download&p=pro&k={%ACF_PRO_KEY}&t={%version}"
                },
                "require": {
                    "composer/installers": "^1.4",
                    "ffraenz/private-composer-installer": "^2.0"
                }
            }
        }
    ],
    "require": {
        "php": ">=7.1",
        "composer/installers": "^1.7"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.4.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
